<table{{ attributes.addClass(classes) }}>
  <tr>
    <th>{% trans %}Name{% endtrans %}</th>
    <th>{% trans %}Downloads{% endtrans %}</th>
  </tr>
  {% for stat in stats %}
    <tr>
      <td>{{ link(stat.title, stat.url, {'target': target}) }}</td>
      <td>{{ stat.downloads }}</td>
    </tr>
  {% endfor %}
</table>
